@page "/create-new-term-main-dictionary"
@using RazorLibrary.Services.Interfaces
@inject NavigationManager NavigationManager
@inject IAppHttpClient AppHttpClient

@code {
    protected override async Task OnInitializedAsync()
    {
        var mainDictionaryIdString = await AppHttpClient.ApiClient.GetStringAsync("TermsDictionary/GetMainDictionaryId");

        NavigationManager.NavigateTo($"/dictionary/{mainDictionaryIdString}/create-new-term");
    }

}
